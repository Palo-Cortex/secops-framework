{
  "soc-isolate-endpoint": {
    "responses": {
      "Cortex Core - IR": {
        "command": "core-isolate-endpoint",
        "inline_args": {
          "endpoint_id": "SOCFramework.Artifacts.EndPointID"
        }
      },
      "CrowdstrikeFalcon": {
        "command": "cs-falcon-contain-host",
        "inline_args": {
          "agent_id(s)": "SOCFramework.Artifacts.EndPointID"
        }
      },
      "Trend Micro Vision One V3": {
        "command": "trendmicro-visionone-isolate-endpoint",
        "inline_args": {
